          <geom name="laser_cylinder_link_visual" pos="0 0 0" quat="1.0 0.0 0.0 0.0" material="black" type="cylinder" size="0.01 0.05" class="visual" />
          <body name="laser_link" pos="0 0 0.05" quat="1.0 0.0 0.0 0.0">
            <inertial mass="0.03" diaginertia="4e-06 4e-06 6e-06" />
            <geom name="laser_link_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="cylinder" size="0.02 0.01" class="collision" />
            <geom name="laser_link_visual" pos="0 0 0" quat="1.0 0.0 0.0 0.0" material="black" type="cylinder" size="0.02 0.01" class="visual" />
          </body>
        </body>
        <body name="camera_link" pos="0.10 0 0.075" quat="1.0 0.0 0.0 0.0">
          <inertial mass="0.01" diaginertia="8.666666666666668e-06 6.666666666666667e-07 8.666666666666668e-06" />
          <geom name="camera_link_collision" pos="0 0 0.0" quat="1.0 0.0 0.0 0.0" type="box" size="0.01 0.05 0.01" class="collision" />
          <geom name="camera_link_visual" pos="0 0 0.0" quat="1.0 0.0 0.0 0.0" material="black" type="box" size="0.01 0.05 0.01" class="visual" />
          <body name="camera_optical_link" pos="0 0 0" quat="0.5000000000000001 -0.5 0.4999999999999999 -0.5" />
        </body>
        <body name="left_wheel_link" pos="0 0.10 -0.06" quat="1.0 0.0 0.0 0.0">
          <joint name="left_wheel_joint" type="hinge" ref="0.0" class="motor" axis="0 1 0" />
          <inertial mass="0.1" diaginertia="3.8933333333333336e-05 3.8933333333333336e-05 5.12e-05" />
          <geom name="left_wheel_link_collision" pos="0 0 0" quat="0.7071090180427968 0.7071045443232221 0.0 0.0" type="cylinder" size="0.032 0.02" class="collision" />
          <geom name="left_wheel_link_visual" pos="0 0 0" quat="0.7071090180427968 0.7071045443232221 0.0 0.0" material="black" type="cylinder" size="0.032 0.02" class="visual" />
        </body>
        <body name="right_wheel_link" pos="0 -0.10 -0.06" quat="1.0 0.0 0.0 0.0">
          <joint name="right_wheel_joint" type="hinge" ref="0.0" class="motor" axis="0 1 0" />
          <inertial mass="0.1" diaginertia="3.8933333333333336e-05 3.8933333333333336e-05 5.12e-05" />
          <geom name="right_wheel_link_collision" pos="0 0 0" quat="0.7071090180427968 0.7071045443232221 0.0 0.0" type="cylinder" size="0.032 0.02" class="collision" />
          <geom name="right_wheel_link_visual" pos="0 0 0" quat="0.7071090180427968 0.7071045443232221 0.0 0.0" material="black" type="cylinder" size="0.032 0.02" class="visual" />
        </body>
        <body name="front_caster_link" pos="0.08 0.0 -0.076" quat="1.0 0.0 0.0 0.0">
          <inertial mass="0.01" diaginertia="1.024e-06 1.024e-06 1.024e-06" />
          <geom name="front_caster_link_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="sphere" size="0.016" class="collision" />
          <geom name="front_caster_link_visual" pos="0 0 0" quat="1.0 0.0 0.0 0.0" material="black" type="sphere" size="0.016" class="visual" />
        </body>
        <body name="back_caster_link" pos="-0.08 0.0 -0.076" quat="1.0 0.0 0.0 0.0">
          <inertial mass="0.01" diaginertia="1.024e-06 1.024e-06 1.024e-06" />
          <geom name="back_caster_link_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="sphere" size="0.016" class="collision" />
          <geom name="back_caster_link_visual" pos="0 0 0" quat="1.0 0.0 0.0 0.0" material="black" type="sphere" size="0.016" class="visual" />
        </body>
      </body>
      <site name="base_footprint_site" pos="0 0 0" quat="1 0 0 0" />
      <camera name="front_camera" mode="track" fovy="90.0" quat="4.329780281177467e-17 4.329780281177466e-17 0.7071067811865475 0.7071067811865476" pos="0.0 2.0 0.5" />
      <camera name="side_camera" mode="track" fovy="90.0" quat="-0.5 -0.4999999999999999 0.5 0.5000000000000001" pos="-2.0 0.0 0.5" />
    </body>
  </worldbody>

  <actuator>
    <motor name="left_wheel_joint_ctrl" joint="left_wheel_joint" class="motor" />
    <motor name="right_wheel_joint_ctrl" joint="right_wheel_joint" class="motor" />
  </actuator>

  <contact>
    <exclude body1="base_link" body2="imu_link" />
    <exclude body1="base_link" body2="laser_cylinder_link" />
    <exclude body1="laser_cylinder_link" body2="laser_link" />
    <exclude body1="base_link" body2="camera_link" />
    <exclude body1="base_link" body2="left_wheel_link" />
    <exclude body1="base_link" body2="right_wheel_link" />
    <exclude body1="base_link" body2="front_caster_link" />
    <exclude body1="base_link" body2="back_caster_link" />
  </contact>

  <sensor>
    <framepos name="base_footprint_site_pos" objtype="site" objname="base_footprint_site" />
    <framequat name="base_footprint_site_quat" objtype="site" objname="base_footprint_site" />
    <framelinvel name="base_footprint_site_linvel" objtype="site" objname="base_footprint_site" />
    <frameangvel name="base_footprint_site_angvel" objtype="site" objname="base_footprint_site" />
    <velocimeter name="base_footprint_site_vel" site="base_footprint_site" />
  </sensor>
</mujoco>