<mujoco model="voxelsky_robot">
  <default>
    <default class="robot">
      <default class="motor">
        <joint />
        <motor />
      </default>
      <default class="visual">
        <geom material="visualgeom" contype="0" conaffinity="0" group="2" />
      </default>
      <default class="collision">
        <geom material="collision_material" condim="3" contype="0" conaffinity="1" priority="1" group="1" solref="0.005 1" friction="1 0.01 0.01" />
        <equality solimp="0.99 0.999 1e-05" solref="0.005 1" />
      </default>
    </default>
  </default>

  <compiler angle="radian" />

  <asset>
    <material name="blue" rgba="0.1 0.1 1.0 0.5" />
    <material name="black" rgba="0.0 0.0 0.0 0.8" />
    <material name="default_material" rgba="0.7 0.7 0.7 1" />
    <material name="collision_material" rgba="1.0 0.28 0.1 0.9" />
  </asset>

  <worldbody>
    <body name="base_footprint" pos="0.00000000 0.00000000 0.00100000" quat="1 0 0 0" childclass="robot">
      <freejoint name="floating_base" />
      <body name="base_link" pos="0.0 0.0 0.091" quat="1.0 0.0 0.0 0.0">
        <inertial mass="1.0" diaginertia="0.0037000000000000006 0.0037000000000000006 0.005000000000000001" />
        <geom name="base_link_collision" pos="0 0 0.0" quat="1.0 0.0 0.0 0.0" type="cylinder" size="0.12 0.06" class="collision" />
        <geom name="base_link_visual" pos="0 0 0.0" quat="1.0 0.0 0.0 0.0" material="blue" type="cylinder" size="0.12 0.06" class="visual" />
        <body name="imu_link" pos="0 0 0.02" quat="1.0 0.0 0.0 0.0">
          <inertial mass="0.01" diaginertia="6.666666666666667e-07 6.666666666666667e-07 6.666666666666667e-07" />
          <geom name="imu_link_collision" pos="0 0 00" quat="1.0 0.0 0.0 0.0" type="box" size="0.01 0.01 0.01" class="collision" />
          <geom name="imu_link_visual" pos="0 0 00" quat="1.0 0.0 0.0 0.0" material="black" type="box" size="0.01 0.01 0.01" class="visual" />
        </body>
        <body name="laser_cylinder_link" pos="0 0 0.10" quat="1.0 0.0 0.0 0.0">
          <inertial mass="0.01" diaginertia="8.583333333333335e-06 8.583333333333335e-06 5.000000000000001e-07" />
